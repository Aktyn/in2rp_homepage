{
  "name": "in2rp_homepage",
  "version": "19.02.15",
  "description": "Homepage project",
  "main": "src/index.tsx",
  "scripts": {
    "page:dev": "webpack-dev-server --config ./webpack.config.js",
    "page:build": "rm -rf dist && webpack --config ./webpack.config.js",
    "page:publish": "rm -rf dist && NODE_ENV='production' webpack --config ./webpack.config.js -p",
    "server:dev": "NODE_ENV=dev nodemon -e js,ts --watch server_src/**/* --exec \"tsc -p ./server_src/tsconfig.json && node server_dist/index.js\"",
    "server:publish": "NODE_ENV=prod tsc -p ./server_src/tsconfig.json",
    "server:run": "NODE_ENV=prod node server_dist/index.js",
    "tools:publish": "mkdir tools && g++ -O3 tools_src/rcon.cpp -o tools/rcon"
  },
